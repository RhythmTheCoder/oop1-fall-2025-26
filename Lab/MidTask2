public class MidTask2 {
    public static void main(String[] args) {
        String firstName= "Anan";
        String lastName= "Farabi";
        String productName= "Book";
        String productId= "6742069";
        String email= "farabi@gmail.com";

        String fullName = firstName + " " + lastName;
        System.out.printIn("Full name:" + fullName);

        String productCode = productName + " " + productId;
        System.out.printIn("Product Code:" + productCode);

        String username = email.substring(0, email.indexOf("@"));

        System.out.println("Username: " + username);

        
    }
    }